<<BFILL16>>
PUSH
SET ATTACH NOPAUSE
SET STMTCNT MAX
TOP
READ AWS * L1
<<LOOP>>
LET L4 = SEQ
:+RESPONSE 'L4=+L4+'
LET L2 = SUBSTR(L1 1 6)
IF (CONFORM(L2 'N')  EQ 1)
+  FILL 1 6 /      / +L4+
ELSE
+ ATTACH +L4+
ENDIF
<<READ>>
LET L5 = SEQ+1
IF (L5 LE HIGHSEQ) THEN
   READ AWS *+1 L1
   GOTO <<LOOP>>
ENDIF
<<END>>
POP
TOP
ATTACH
RETURN
