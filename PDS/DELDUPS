<<DELDUPS>> : A1=COL, A2=LEN
IF A1 EQ ''
    WRITE ' '
    WRITE 'ENTER COMPARE COLUMN'
    READ L4
ELSE
    LET L4 = A1
ENDIF
IF A2 EQ ''
    WRITE ' '
    WRITE 'ENTER COMPARE LENGTH'
    READ L5
ELSE
    LET L5 = A2
ENDIF
SET STMTCNT MAX
RENUMBER 1 1
READ AWS 1 L3
LET L2 = TRIM(SUBSTR(L3 L4 L5))
LOOP L1 FROM 2 TO HIGHSEQ BY 1
  +READ AWS +L1+ L3
  LET L3 = TRIM(SUBSTR(L3 L4 L5))
  IF L2 EQ L3
      +DELETE +L1+
  ELSE
      LET L2 = L3
  ENDIF
ENDLOOP
RETURN
