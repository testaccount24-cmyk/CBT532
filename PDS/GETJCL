<<GETJCL>>
:L1 MEMBER NAME
  PUSH
  DELETE
  SET MODE NUMX
  PANEL ACTIVATE *
  STARTDEF
  TAG ! S
   TAG % UH MEM LIB1 LIB2 LIB3
   CURSOR MEM
!
!                         IMPORT A MEMBER
!                      FROM HLQ.DVL.JCLLIB
!
!             ENTER PDS MEMBER NAME: %________!
!
!
!
!
!
!
!
!
!
!
!
!
!
!
!
!
!
!
!
  ENDDEF
  PANEL SEND
  SET MODE BASIC

<<READ>>
  IF AID EQ 'CLEAR'
     GOTO <<END>>
  ENDIF
  LET L1 = TRIM(TRIM(P.MEM) '_')
  IF L1 EQ ' '
    GOTO <<END>>
  ENDIF
+IMPORT DSN=HLQ.DVL.JCLLIB(+L1+)
 <<ENDIT>>
  SET ATTACH NOPAUSE
  ATTACH
  NOSEQ
  POP
  STOP
 <<END>>
  WRITE '     GETJCL EXITED   '
  NOSEQ
  POP
  STOP
