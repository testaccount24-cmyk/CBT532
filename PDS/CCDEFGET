<<CCDEFGET>> : A1=PARM NAME
  PUSH
  DECLARE CARD PTR V<8>
  ATTACH AWS CCDEFS
  SET BOUNDS AWS 1 4
  LOOP PTR FROM 1 TO 8 BY 1
    UNTIL A<PTR> EQ ''
      TRAP
     %FIND /%A<PTR>%/
      IF TC EQ 0
         %READ AWS %SEQ% CARD
          LET V<PTR> = TRIM(SUBSTR(CARD 8))
      ENDIF
  ENDLOOP
  TRAP OFF
  SET BOUNDS AWS 1 72
  DETACH
  POP
  RETURN V<1> V<2> V<3> V<4> V<5> V<6> V<7> V<8>
