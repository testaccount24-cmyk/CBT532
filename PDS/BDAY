<<BDAY>>
PUSH
LET L1 = DATEFUN('AC')
LET L5 = SUBSTR(L1 7 2)
FETCH  PFX.BDAYS
RENUMBER 1,1
+FILL 7 8 /+L5+/
POINT
TRAP ON
LOOP S.HIGHSEQ TIMES
    READ AWS * L2
    LET L3 = SUBSTR(L2 1 8)
    +LET L4 = DATEFUN('DRAA' '+L1+' '+L3+')
    IF (L4 LT 11) AND (L4 GT 0)
        ESCAPE
    ENDIF
    POINT *+1
ENDLOOP
TRAP OFF
POP
DELETE
IF (L4 LT 11) AND (L4 GT 0)
    ATTACH PFX.BDAYS
ENDIF
RETURN
