<<RANDVL>>
:L1 RANGE SEARCH STRING
  PUSH
  DELETE
  SET MODE NUMX
  PANEL ACTIVATE *
  STARTDEF
  TAG ! S
   TAG % UH MEM
   CURSOR MEM
!***********************************************************************
!**                          S E A R C H                              **
!**              D V L  S A T  P R O D  S Y S 1 A N D S Y S 2         **
!**              P R O C   L I B R A R I E S                          **
!***********************************************************************
!
!                  PLEASE SPECIFY SEARCH CRITERIA
!                                OR
!                        PRESS CLEAR TO EXIT
!
!
!                 MEMBER ===========> %________!
!
!
!
!***********************************************************************
  ENDDEF
  PANEL SEND
  SET MODE BASIC
  TRAP ON
  DELETE DEVAWS
  DELETE SATAWS
  DELETE PRDAWS
  DELETE SY1AWS
  DELETE SY2AWS
  DELETE SY2AWS1
  DELETE SY1AWS1
  DELETE SATAWS2
  DELETE SY3AWS
  DELETE USCAWS
  TRAP OFF

<<READ>>
  IF AID EQ 'CLEAR'
     GOTO <<END>>
  ENDIF
  LET L1 = TRIM(TRIM(P.MEM) '_')
  IF L1 EQ ' '
    GOTO <<RANDVL>>
    ENDIF
  +IMPORT DSN=SYS3.DVL.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '************ DEVELOPMENT ************'
  SAVE DEVAWS
  DELETE
  +IMPORT DSN=SYS3.SAT.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '************ S.A.T.************'
  SAVE SATAWS
  DELETE
::+IMPORT DSN=SYS3.PROD.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '************ PROD ************'
  SAVE PRDAWS
  DELETE
::+IMPORT DSN=SYS1.JCLLIB(*),RANGE='+L1+'
  WRITE AWS T '********SYS1.JCLLIB***********'
  SAVE SY1AWS
  DELETE
::+IMPORT DSN=SYS2.MVS.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '********SYS2.MVS.PROCLIB******'
  SAVE SY2AWS
  DELETE
  +IMPORT DSN=SYS2.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '********SYS2.PROCLIB**********'
  SAVE SY2AWS1
  DELETE
  +IMPORT DSN=SYS1.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '********SYS1.PROCLIB**********'
  SAVE SY1AWS1
  DELETE
  +IMPORT DSN=SYS2.SAT.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '********SYS2.SAT.PROCLIB**********'
  SAVE SATAWS2
  DELETE
  +IMPORT DSN=SYS3.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '********SYS3.PROCLIB**********'
  SAVE SY3AWS
  DELETE
  +IMPORT DSN=HLQ.PROD.PROCLIB(*),RANGE='+L1+'
  WRITE AWS T '********HLQ.PROD.PROCLIB**********'
  SAVE USCAWS
  DELETE
  COPY DEVAWS 1 999999 T
  COPY SATAWS 1 999999 B
  COPY PRDAWS 1 999999 B
  COPY SY1AWS 1 999999 B
  COPY SY2AWS 1 999999 B
  COPY SY2AWS1 1 999999 B
  COPY SY1AWS1 1 999999 B
  COPY SATAWS2 1 999999 B
  COPY SY3AWS 1 999999 B
  COPY USCAWS 1 999999 B
  DELETE DEVAWS
  DELETE SATAWS
  DELETE PRDAWS
  DELETE SY1AWS
  DELETE SY2AWS
  DELETE SY2AWS1
  DELETE SY1AWS1
  DELETE SATAWS2
  DELETE SY3AWS
  DELETE USCAWS
  SET ATTACH NOPAUSE
  ATTACH
  NOSEQ
  POP
  STOP
 <<END>>
  WRITE '     LIBRARY SEARCH EXITED   '
  NOSEQ
  POP
  STOP
