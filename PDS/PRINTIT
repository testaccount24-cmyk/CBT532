<<PRINTIT1>>
PUSH
CREATE AWS $DANA$
SELECT AWS $DANA$
COPY PRINTIT1 100 1600 T
+COPY +A1+ B
SET MASTER OFF
SUB
SET MASTER ON
DISCARD AWS $DANA$
POP
RETURN
