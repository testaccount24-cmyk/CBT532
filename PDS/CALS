<<CALS>>
:
:COMPUTE CALORIES TO MAINTAIN WEIGHT
:A DEFICIT OF 3500 CALORIES GIVE A WEIGHT LOSS OF 1 LB
:
IF (A1 EQ '')
   RESPONSE 'CALS LBS'
   RETURN
ENDIF
IF (A1 LT 121)
   RESPONSE 'MINIMUM LBS IS 121'
   RETURN
ENDIF
LET L1 = A1
LET L2 = L1 - 121
LET L3 = 2000 + (L2 * 10)
+RESPONSE 'IT TAKES +L3+ CALORIES TO MAINTAIN +L1+ LBS'
<<END>>
RETURN
