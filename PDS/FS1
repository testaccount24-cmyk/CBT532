TOP
DECLARE PTR AWSLINE
READ AWS * AWSLINE
TRAP ON
LOOP
    UNTIL TC NE 0  OR  S.AID EQ 'CLEAR'
    PANEL ACTIVATE *
STARTDEF
TAG # S LINE
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
#
ENDDEF
    LOOP PTR FROM 1 TO 24 BY 1
        LET P.LINE<PTR> = SUBSTR(AWSLINE 1 79)
        POINT *+1
        IF TC NE 0
            ESCAPE
        ENDIF
        READ AWS * AWSLINE
    ENDLOOP
    PANEL SEND
ENDLOOP
TRAP OFF
