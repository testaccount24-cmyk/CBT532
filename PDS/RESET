<<RESET>> : A1=EDIT LENGTH
IF A1 EQ ''
    LET L1 = 72
ELSE
    LET L1 = A1
ENDIF
SET ;
NOSEQ
%SET BOUNDS AWS 1 %L1%
DETACH LIB
SET ATTACH NOPAUSE
SET EDITCHNG OFF
SET USERID OFF
: SET CMDLINES 2 1 CLEAR
SET DSNCNT MAX
SET DSNEMPTY ON
SET PFKDELIM ;
SET TLOCK OFF
SET DISPLAY 1 72
SET SUBMIT NOSEQ
SET SUBMIT NOINCLUDE
SET MSGLEVEL INFO
SET PFKTRAP ON
SET MODE BASIC
SET MODE NUMX
SET AUTOFF MAX
SET DDI OFF
SET MASTER OFF
SET SCROLL FULL
SET EOF DELETE1
SET HEX OFF
SET FILL NULL
: SET JOBCNT MAX
RETURN
