<<FINDPGM>> :RANGE SEARCH RPF FOR ALL LIBRARIES
:L1 RANGE SEARCH STRING
  PUSH
  DELETE
  SET MODE NUMX
  PANEL ACTIVATE *
  STARTDEF
  TAG ! S
  TAG % UH MEM
  TAG ¢ (SH RED)
  TAG # (SH YELLOW)
   CURSOR MEM
!***********************************************************************
!**                        #RANGE SEARCH!                             **
!**                  #D V L  &  S A T   &  P R O D!                   **
!**                             #FOR!                                 **
!**                     ¢NON-Y2K & Y2K & ENDEVOR!                     **
!***********************************************************************
!
!                    PLEASE SPECIFY PROGRAM NAME
!                     (ALL OR PART OF THE NAME)
!                                OR
!                        PRESS CLEAR TO EXIT
!
!
!                #MODULE NAME! =====> %________!
!
!
!
!
  ENDDEF
  PANEL SEND
  SET MODE BASIC

<<READ>>
  IF AID EQ 'CLEAR'
     GOTO <<END>>
  ENDIF
  LET L1 = TRIM(TRIM(P.MEM) '_')
  IF L1 EQ ' '
    GOTO <<FINDPGM>>
    ENDIF
  SET MONTRAP ON
  +IMPORT DSN=DVL.SOURCE.LIBMAST(*),RANGE='+L1+'
   WRITE AWS T '*******  NON-Y2K DEVELOPMENT *******'
   WRITE AWS T '                                    '
   WRITE AWS T '============   NON-Y2K MASTER FILES  ============'
  SAVE DEVAWS
  DELETE
  +IMPORT DSN=SAT.SOURCE.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  NON-Y2K S.A.T.      *******'
  SAVE SATAWS
  DELETE
  +IMPORT DSN=ASD.SAT.SOURCE.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  NON-Y2K S.A.T.ADMIN *******'
  SAVE SADAWS
  DELETE
  +IMPORT DSN=PROD.SOURCE.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  NON-Y2K PRODUCTION  *******'
  SAVE PRDAWS
  DELETE
  +IMPORT DSN=HLQ.DVL.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ACS Y2K DEVELOPMENT *******'
  WRITE AWS T '                                    '
  WRITE AWS T '============   Y2K MASTER FILES      ============'
  WRITE AWS T '                                    '
  SAVE ACDAWS
  DELETE
  +IMPORT DSN=HLQ.SAT.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ACS Y2K S.A.T.      *******'
  SAVE ACSAWS
  DELETE
  +IMPORT DSN=HLQ.PRD.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ACS Y2K PRODUCTION  *******'
  SAVE ACPAWS
  DELETE
  +IMPORT DSN=HLQ.ADM.DVL.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ADM Y2K DEVELOPMENT *******'
  SAVE ADDAWS
  DELETE
  +IMPORT DSN=HLQ.ADM.SAT.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ADM Y2K S.A.T.      *******'
  SAVE ADSAWS
  DELETE
  +IMPORT DSN=HLQ.ADM.PRD.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ADM Y2K PRODUCTION  *******'
  SAVE ADPAWS
  DELETE
  +IMPORT DSN=BSDENDV.DVLMUNT.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ENDEVOR STAGE 8 DVLMUNT *******'
  WRITE AWS T '                                    '
  WRITE AWS T '============   ENDEVOR MASTER FILES  ============'
  WRITE AWS T '                                    '
  SAVE MUNTAWS
  DELETE
  +IMPORT DSN=BSDENDV.DVLMSYS.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ENDEVOR STAGE 7 DVLMSYS *******'
  SAVE MSYSAWS
  DELETE
  +IMPORT DSN=BSDENDV.SATMSTG.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ENDEVOR STAGE 6 SATMSTG *******'
  SAVE MSTGAWS
  DELETE
  +IMPORT DSN=BSDENDV.SATMTST.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ENDEVOR STAGE 5 SATMTST *******'
  SAVE MTSTAWS
  DELETE
  +IMPORT DSN=BSDENDV.PRDPRD.LIBMAST(*),RANGE='+L1+'
  WRITE AWS T '*******  ENDEVOR STAGE 1 PRDPRD  *******'
  SAVE PRDPAWS
  DELETE
  SET MONTRAP OFF
  COPY DEVAWS 1 999999 T
  COPY SATAWS 1 999999 B
  COPY SADAWS 1 999999 B
  COPY PRDAWS 1 999999 B
  COPY ACDAWS 1 999999 B
  COPY ACSAWS 1 999999 B
  COPY ACPAWS 1 999999 B
  COPY ADDAWS 1 999999 B
  COPY ADSAWS 1 999999 B
  COPY ADPAWS 1 999999 B
  COPY MUNTAWS 1 999999 B
  COPY MSYSAWS 1 999999 B
  COPY MSTGAWS 1 999999 B
  COPY MTSTAWS 1 999999 B
  COPY PRDPAWS 1 999999 B
  DELETE DEVAWS
  DELETE SATAWS
  DELETE SADAWS
  DELETE PRDAWS
  DELETE ACDAWS
  DELETE ACSAWS
  DELETE ACPAWS
  DELETE ADDAWS
  DELETE ADSAWS
  DELETE ADPAWS
  DELETE MUNTAWS
  DELETE MSYSAWS
  DELETE MSTGAWS
  DELETE MTSTAWS
  DELETE PRDPAWS
  SET ATTACH NOPAUSE
  ATTACH
  NOSEQ
  POP
  STOP
 <<END>>
  WRITE '     LIBRARY SEARCH EXITED   '
  NOSEQ
  POP
  STOP
