<<GETY2K>>
:L1 MEMBER NAME
  PUSH
  DELETE
  SET MODE NUMX
  PANEL ACTIVATE *
  STARTDEF
  TAG ! S
   TAG % UH MEM LIB1 LIB2 LIB3
   CURSOR MEM
!
!                     IMPORT A Y2K MEMBER
!
!
!             ENTER MEMBER NAME: %________!
!
!
!             PLACE AN 'X' BESIDE DESIRED FILE
!
!
!             HLQ.DVL.LIBMAST: %_!
!             HLQ.SAT.LIBMAST: %_!
!             HLQ.PRD.LIBMAST: %_!
!
!
!
!
!
!
!
!
!
!
!
  ENDDEF
  PANEL SEND
  SET MODE BASIC

<<READ>>
  IF AID EQ 'CLEAR'
     GOTO <<END>>
  ENDIF
  LET L1 = TRIM(TRIM(P.MEM) '_')
  IF L1 EQ ' '
    GOTO <<END>>
  ENDIF
  IF (P.LIB1 EQ 'X')
     +IMPORT DSN=HLQ.DVL.LIBMAST(+L1+)
      GOTO <<ENDIT>>
  ENDIF
  IF (P.LIB2 EQ 'X')
     +IMPORT DSN=HLQ.SAT.LIBMAST(+L1+)
      GOTO <<ENDIT>>
  ENDIF
  IF (P.LIB3 EQ 'X')
     +IMPORT DSN=HLQ.PRD.LIBMAST(+L1+)
      GOTO <<ENDIT>>
  ENDIF
 <<ENDIT>>
  SET ATTACH NOPAUSE
  ATTACH
  NOSEQ
  POP
  STOP
 <<END>>
  WRITE '     GETY2K EXITED   '
  NOSEQ
  POP
  STOP
