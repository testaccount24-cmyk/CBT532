<<JCNVRT>>
PUSH
DECLARE CARD1 CARD2 CARD3 CARD4
CREATE AWS JCNVRT NONRECOVERABLE LEVEL
SELECT AWS JCNVRT
TRAP
FETCH $$JPARMS
IF TC NE 0
    TRAP
    FETCH CBL2USER
    TRAP OFF
    IF TC NE 0
        1                LOCAL
        2 DVL.SOURCE.LIBMAST                       ASD
        3 CICS.OMS.APPLOAD                             31 ANY OPT
        4 HLQ.ACS.DVL.BATLOAD                             31 ANY OPT
    ENDIF
    READ AWS 1 CARD1
    READ AWS 2 CARD2
    READ AWS 3 CARD3
    READ AWS 4 CARD4
    DELETE
    %01 JNAM = D%SUBSTR(CARD1 1 4)%
    %02 ACCT = %SUBSTR(CARD1 6 4)%
    %03 ROOM = %SUBSTR(CARD1 11 5)%
    %04 UNAM = %SUBSTR(CARD1 25 11)%
     05 MCLS = S
    %06 PRTR = %SUBSTR(CARD1 16 8)%
    %07 DIVN = %SUBSTR(CARD2 42 3)%
    %08 SRCE = %SUBSTR(CARD2 1 34)%
    %09 CCOP = %SUBSTR(CARD3 53 20)%
    %10 BCOP = %SUBSTR(CARD4 53 20)%
    %11 CLNK = %SUBSTR(CARD3 1 34)%
    %12 BLNK = %SUBSTR(CARD4 1 34)%
    %13 CLOP = AMODE=%SUBSTR(CARD3 45 2)%,RMODE=%SUBSTR(CARD3 49 3)%
    %14 BLOP = AMODE=%SUBSTR(CARD4 45 2)%,RMODE=%SUBSTR(CARD4 49 3)%
ENDIF
RENUMBER 1 1
UPDATE $$JPARMS
DISCARD AWS JCNVRT
POP
RETURN
