<<LOUP>>
:
: LOUP STARTLINE ENDLINE
:
PUSH
IF (A1 EQ '')
   RESPONSE 'LOUP START END'
   RETURN
ENDIF
IF (A2 EQ '')
   RESPONSE 'LOUP START END'
   RETURN
ENDIF
SET ATTACH NOPAUSE
LET L16 = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'
LET L15 = 'abcdefghijklmnopqrstuvwxyz'
+ATTACH +A1+,,
+IF (+A2+ LT SEQ)
    RETURN
 ENDIF
READ AWS * L1
<<LOOP>>
LET L4 = SEQ
LET L2 = XLATE(L1 L15 L16)
+ +L4+ +L2+
<<READ>>
LET L5 = SEQ + 1
+IF (L5 LE +A2+) THEN
   READ AWS *+1 L1
   GOTO <<LOOP>>
ENDIF
<<END>>
POP
RETURN
