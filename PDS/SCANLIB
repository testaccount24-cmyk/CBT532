<<SCANLIB>>
:
:SCANLIB LIBNAME SEARCHSTRING SAVENAME RANGE
:SCANLIB 'PROD.SOURCE.LIBMAST' STRING MIST CPPX
:        'DVL.SOURCE.LIBMAST'
:        'SAT.SOURCE.LIBMAST'
:        'SYS1.MVS.LIBMAST'
:
PUSH
+IMPORT DSN=+A1+(*),RANGE='+A4+'
DELETES /LAST-UPDATE/
FILL 11 255 / /
LET L1 = LENGTH(A1)
IF (L1 EQ 2)
   LSHIFT 1
ENDIF
SORT SORT FIELDS=(1,12,CH,D)
+SUFFIX /)' '+A2+'/
+PREFIX /EXEC SCANLMEM '+A1+(/
RENUMBER
+1 <<+A3+>>
+SAVE +A3+
