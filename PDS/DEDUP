<<DEDUP>>
:
: DEDUP STARTCOL ENDCOL
:
PUSH
TOP
TRAP ON
SET ATTACH NOPAUSE
READ AWS * L1                             : L1 IS COPY OF LINE
LET L2 = A2 - A1 + 1                      : L2 IS LENGTH
<<LOOP>>
+LET L3 = SUBSTR(L1 +A1+ +L2+)            : L3 IS SUBSTR OF LINE
LET L4 = SEQ
LET L5 = SEQ + 1
IF (L5 LE HIGHSEQ) THEN
+  DELETES +A1+ +A2+ /+L3+/ +L5+ ,,
+  ATTACH +L4+
   READ AWS *+1 L1
   GOTO <<LOOP>>
ENDIF
<<END>>
TOP
ATTACH
TRAP OFF
POP
RETURN
